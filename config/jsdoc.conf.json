{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src", "config/tests/utils"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true,
    "default": {}
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "readme": "README.md",
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "title": "Documentation",
      "menu": [
        {
          "title": "Demo",
          "link": "https://creact.netlify.app",
          "target": "_blank"
        },
        {
          "title": "GitHub",
          "link": "https://github.com/Joshua-Booth/creact/",
          "target": "_blank"
        }
      ],
      "meta": [
        {
          "name": "author",
          "content": "Joshua Booth"
        },
        {
          "name": "description",
          "content": "A project template for creating awesome React web apps."
        }
      ],
      "search": false,
      "footer": "Copyright Â© Joshua Booth",
      "resizeable": {
        "navbar": {
          "min": "300",
          "max": "600"
        }
      }
    }
  }
}
